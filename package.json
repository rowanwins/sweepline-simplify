{
  "name": "sweepline-simplify",
  "version": "0.0.1",
  "description": "A module for creating simple polygons out of self-intersecting polygons",
  "main": "dist/sweeplineSimplify.js",
  "module": "src/main.js",
  "unpkg": "dist/sweeplineSimplify.min.js",
  "jsdelivr": "dist/sweeplineSimplify.min.js",
  "scripts": {
    "bench": "npm run build && node test/benchmark.js",
    "build": "rollup -c",
    "debug": "cross-env webpack-dev-server --config debug/webpack.config.js --mode development --open --hot",
    "test": "ava --verbose",
    "test:e2e": "ava ./test/test.spec.js --verbose",
    "test:coverage": "nyc ava"
  },
  "ava": {
    "files": [
      "test/*.spec.js"
    ],
    "require": [
      "esm"
    ]
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "tinyqueue": "^2.0.3",
    "write-json-file": "^4.2.0"
  },
  "devDependencies": {
    "ava": "^1.0.1",
    "benchmark": "^2.1.4",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "eslint": "^5.12.0",
    "eslint-config-mourner": "^3.0.0",
    "esm": "^3.1.0",
    "file-loader": "^3.0.1",
    "geojson-polygon-self-intersections": "^1.2.0",
    "glob": "^7.1.3",
    "json-loader": "^0.5.7",
    "leaflet": "^1.4.0",
    "load-json-file": "^5.1.0",
    "martinez": "0.0.6",
    "martinez-polygon-clipping": "^0.5.0",
    "nyc": "^13.1.0",
    "rollup": "^1.1.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-strip": "^1.2.1",
    "rollup-plugin-terser": "^4.0.2",
    "serve": "^10.1.1",
    "simplepolygon": "^1.2.3",
    "vue": "^2.5.22",
    "vue-loader": "^15.6.2",
    "vue-template-compiler": "^2.5.22",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  }
}
